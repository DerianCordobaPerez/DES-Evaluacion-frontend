@{
    ViewData["Title"] = "DES-Evaluacion-Frontend";
}

@section Styles
{
    <link rel="stylesheet" href="~/css/site.css"/>
}

@section Header
{
    <header class="header">
        <div class="header-content">
            <h3 class="header-title">An&aacutelisis casos COVID-19 Centroamerica</h3>
            <p class="header-sub-title">Estado pandemia</p>
        </div>
    </header>
}

@section Sidebar
{
    <aside class="sidebar">
        
    </aside>
}

<div class="content-map">
